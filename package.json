{
    "name": "image-shrink",
    "productName": "image-shrink",
    "version": "1.0.0",
    "description": "",
    "main": "main.js",
    "scripts": {
        "start": "electron .",
        "dev": "nodemon --exec electron .",
        "test": "echo \"Error: no test specified\" && exit 1",
        "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
        "package-win": "electron-packager . --overwrite --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=false --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"ImageShrink\" NODE_ENV=prod" ,
        "package-linux": "electron-packager . electron-tutorial-app --overwrite --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=false --out=release-builds"
    },
    "author": "",
    "license": "MIT",
    "devDependencies": {
        "electron": "^10.1.2",
        "electron-packager": "^15.1.0"
    },
    "dependencies": {
        "electron-log": "^4.2.4",
        "imagemin": "^7.0.1",
        "imagemin-mozjpeg": "^9.0.0",
        "imagemin-pngquant": "^9.0.1",
        "slash": "^3.0.0"
    }
}
